{
  "name": "mint_scan-cli",
  "version": "1.0.0",
  "description": "CLI for MintScan API operations using Bun and TypeScript",
  "main": "src/index.ts",
  "type": "module",
  "bin": {
    "mint_scan-cli": "src/index.ts"
  },
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun --watch src/index.ts",
    "build": "bun build src/index.ts --compile --outfile mint_scan-cli",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit"
  },
  "keywords": ["mintscan", "cli", "api", "typescript", "bun"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^12.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  }
}